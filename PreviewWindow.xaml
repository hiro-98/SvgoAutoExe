<Window x:Class="SvgoAutoExe.PreviewWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:SvgoAutoExe" mc:Ignorable="d" Closing="PreviewWindowClosing" Title="Preview" Height="800" Width="800">
    <Grid>
        <WebBrowser x:Name="Browser" Margin="0,0,0,20"/>
        <Grid HorizontalAlignment="Right" Margin="297,659,0,0" VerticalAlignment="Bottom">
            <ToggleButton x:Name="ButtonShowTop" Content="最前面に表示" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="-12,0,0,0" Width="85" Checked="ButtonShowTopChecked" Unchecked="ButtonShowTopUnChecked"/>
        </Grid>
    </Grid>
</Window>
